package templates

templ Index(title string) {
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1 class="text-3xl font-bold underline">Hello, World!</h1>
</body>
</html>
}
